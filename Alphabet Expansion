#include <iostream>
#include<string> 
using namespace std;

int main()
{
string s;
cin>>s;
string s2="";
char c;
int n=s.length();
int k=0;
int n1=0;
for(int i=0;i<n;i++)
{
    if(isdigit(s[i]))
    {
        s2=s2+s[i];
        
        
    }
    else if(isalpha(s[i]))
    {
        n1=stoi(s2);
        c=s[i];
        for(int j=0;j<n1;j++)
        cout<<c;
        s2.clear();
        
    }
    
}
return 0;
}
